pipeline {
    agent any

  stages {
    stage('Build') {
      steps {
        script {
          sh 'echo "Building Stage"'
        }
      }
    }

    stage('Test') {
      steps {
        script {
          sh 'echo "Testing Stage"'
        }
      }
    }

    stage ('Test Nested Stages') {
      stages{
        stage('Nested') {
          steps{
            sh 'echo "Nested Stage"'
          }
        }
      }

    }

    stage('Package') {
      steps{
        script {
          sh 'echo "Testing Package"'
        }
      }
    }

    stage('Deploy') {
      steps {
        script {
          sh 'echo "Testing Deploy"'
        }
      }
    }

    stage('Clean Up') {
      steps {
        script {
          sh 'echo "Testing Clean Up"'
        }
      }
    }
    }
}